        {
          "alias": false,
          "_styles": {
            "& when not (@fullScreen)": {
              "padding-top": "(@paddingTop * 1rem)",
              "padding-bottom": "(@paddingBottom * 1rem)"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)"
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@fullWidth)": {
              ".content": {
                "margin-left": "6rem",
                "@media (max-width: 1199px)": {
                  "margin-left": "35px"
                },
                "@media (max-width: 959px)": {
                  "margin-left": "auto",
                  "margin-right": "auto",
                  "text-align": "center",
                  "& when (@showImages)": {
                    "padding-bottom": "250px",
                    "@media (max-width: 690px)": {
                      "padding-bottom": "0"
                    }
                  }
                }
              }
            },
            "& when (@showTitleAddition)": {
              ".title-wrap": {
                "display": "flex",
                "@media (max-width: 959px)": {
                  "display": "block"
                }
              }
            },
            ".mbr-section-title": {
              "color": "#1f1437"
            },
            ".mbr-title-addition": {
              "color": "#1f1437",
              "align-self": "flex-end",
              "margin-bottom": "20px",
              "span": {
                "position": "relative",
                "svg": {
                  "position": "absolute",
                  "left": "0",
                  "bottom": "-12px",
                  "width": "150%",
                  "height": "9px",
                  "@media (max-width: 959px)": {
                    "left": "-12px"
                  },
                  "path": {
                    "fill": "@strokeColor"
                  },
                  "& when not (@showStroke)": {
                    "display": "none"
                  }
                }
              }
            },
            ".mbr-section-subtitle": {
              "color": "#1f1437",
              "span": {
                "position": "relative",
                "svg": {
                  "position": "absolute",
                  "right": "-50px",
                  "bottom": "50px",
                  "transform": "rotate(20deg)",
                  "width": "68px",
                  "height": "62px",
                  "path": {
                    "fill": "@subDecColor"
                  },
                  "& when not (@subDecoration)": {
                    "display": "none"
                  },
                  "@media (max-width: 599px)": {
                    "display": "none"
                  }
                }
              }
            },
            ".mbr-text": {
              "color": "#1f1437",
              "margin": "40px 0"
            },
            ".mbr-section-btn": {
              "& when (@showButtons)": {
                "display": "flex",
                "flex-wrap": "wrap"
              },
              "@media (max-width: 959px)": {
                "justify-content": "center!important"
              }
            },
            ".right-images": {
              "position": "absolute",
              "width": "65%",
              "height": "100%",
              "top": "0",
              "right": "0",
              "overflow": "hidden",
              "@media (max-width: 959px)": {
                "display": "none"
              }
            },
            ".animated-container": {
              "width": "150%",
              "height": "200%",
              "display": "flex",
              "flex-direction": "column",
              "flex-wrap": "wrap",
              "align-content": "flex-start",
              "transform": "rotate(25deg)"
            },
            ".img": {
              "width": "270px",
              "height": "200px",
              "margin": "0.5rem",
              "background-position": "center",
              "background-repeat": "no-repeat",
              "background-size": "contain"
            },
            ".img1": {
              "background-image": "url(@img1)"
            },
            ".img2": {
              "background-image": "url(@img2)"
            },
            "& when (@imageAnimation)": {
              ".animated": {
                "animation": "cooky 0.4s infinite linear",
                "backface-visibility": "hidden"
              }
            },
            ".bottom-images": {
              "display": "none",
              "@media (max-width: 959px)": {
                "display": "block",
                "position": "absolute",
                "width": "100%",
                "height": "400px",
                "left": "0",
                "bottom": "0",
                "overflow": "hidden"
              },
              "@media (max-width: 690px)": {
                "display": "none"
              }
            },
            ".wave-container": {
              "position": "absolute",
              "width": "100%",
              "height": "100px",
              "left": "0",
              "bottom": "-1px",
              "overflow": "hidden",
              ".wave": {
                "width": "5500px",
                "position": "relative",
                "left": "-3300px",
                "svg": {
                  "width": "100%",
                  "height": "100%",
                  "object-fit": "cover",
                  "& when (@waveAnimation)": {
                    "animation": "wavy 35s infinite linear",
                    "backface-visibility": "hidden"
                  },
                  "path": {
                    "fill": "@waveColor"
                  }
                }
              }
            },
            "@keyframes wavy": {
              "0%": {
                "opacity": "1",
                "transform": "translateX(0px) translateY(0px) rotate(0deg) scale(1, 1)",
                "animation-timing-function": "linear"
              },
              "100%": {
                "opacity": "1",
                "transform": "translateX(3230px) translateY(0px) rotate(0deg) scale(1, 1)"
              }
            },
            "@keyframes cooky": {
              "0%": {
                "opacity": "1",
                "transform": "translateX(0px) translateY(0px) rotate(0deg) scale(1, 1)",
                "animation-timing-function": "linear"
              },
              "25%": {
                "opacity": "1",
                "transform": "translateX(0px) translateY(0px) rotate(5deg) scale(1, 1)",
                "animation-timing-function": "linear"
              },
              "50%": {
                "opacity": "1",
                "transform": "translateX(0px) translateY(0px) rotate(0deg) scale(1, 1)",
                "animation-timing-function": "linear"
              },
              "75%": {
                "opacity": "1",
                "transform": "translateX(0px) translateY(0px) rotate(-5deg) scale(1, 1)",
                "animation-timing-function": "linear"
              },
              "100%": {
                "opacity": "1",
                "transform": "translateX(0px) translateY(0px) rotate(0deg) scale(1, 1)"
              }
            }
          },
          "_name": "header1",
